html
  head
    title Voter Totals page
    meta(name="description" content="A cool thing made with Glitch")
    link#favicon(rel="icon" href="https://glitch.com/edit/favicon-app.ico" type="image/x-icon")
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(rel="stylesheet" href="/style.css")
    link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css")
    link(rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous")
    script(src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous")
    script(src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js" integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU=" crossorigin="anonymous")
    script(src="/client.js" defer)
  
  body
    div.container
      button
        a(href="/logout") Logout
      h1 Voters
      p Number of your supporters who have voted and not voted as of #{date}
      div.row.font-weight-bold
        div.col-1 Precinct
        div.col-1 Dordek
        div.col-1 Jenkins
        div.col-1 Kastafaros
        div.col-1 Ladien
        div.col-1 Maloney
        div.col-1 Martin
        div.col-1 Negron
        div.col-1 Schwartzers
        div.col-1 Kitzes
        div.col-1 Reported by
      hr
      each item, i in arr
        div.row
          div.col-1=item.precinct
          div.col-1=item.dordek
          div.col-1=item.jenkins
          div.col-1=item.kastafaros
          div.col-1=item.ladien
          div.col-1=item.maloney
          div.col-1=item.martin
          div.col-1=item.negron
          div.col-1=item.schwartzers
          div.col-1=item.kitzes
          div.col-1=item.name

        hr

      button
        a(href="/logout") Logout